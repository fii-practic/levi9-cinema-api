{
    "taskRoleArn": "arn:aws:iam::AWS_ACCOUNT_PLACEHOLDER:role/levi9-cinema-api-ecs-service-role",
    "executionRoleArn": "arn:aws:iam::AWS_ACCOUNT_PLACEHOLDER:role/levi9-cinema-api-ecs-task-role",
    "containerDefinitions": [
        {
            "name": "levi9-cinema-api",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "levi9-cinema-api"
}
